{
  "name": "ng-holistic",
  "version": "0.0.25",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start:home": "ng serve hlc-home-app --port=4305",
    "start:clr-common": "ng serve clr-common-sandbox --port=4300",
    "start:clr-forms": "ng serve clr-forms-sandbox --port=4301",
    "start:clr-list": "ng serve clr-list-sandbox --port=4302",
    "build:hlc-home-app": "ng build hlc-home-app",
    "build:clr-common-sandbox-app": "ng build clr-common-sandbox",
    "build:clr-forms-sandbox-app": "ng build clr-forms-sandbox",
    "build:clr-list-sandbox-app": "ng build clr-list-sandbox",
    "build:clr-list-app": "ng build clr-list-sandbox",
    "build:clr-layout": "ng build clr-layout",
    "build:clr-common": "ng build clr-common",
    "build:clr-controls": "ng build clr-controls",
    "build:clr-file-upload": "ng build clr-file-upload",
    "build:clr-forms": "npm run build:forms && npm run build:clr-controls && ng build clr-forms --ts-config=tsconfig.build.json",
    "build:clr-list": "npm run build:clr-forms && ng build clr-list --ts-config=tsconfig.build.json",
    "build:clr-wizard": "npm run build:clr-forms && ng build clr-wizard --ts-config=tsconfig.build.json",
    "build:forms": "ng build forms",
    "build:libs": "npm run build:clr-file-upload && npm run build:clr-forms && npm run build:clr-list",
    "start:prod-local-libs": "ng serve --configuration=production-local-libs",
    "start:list-prod-local-libs": "ng serve clr-list-sandbox --configuration=production-local-libs",
    "start:dev-local-libs": "ng serve --configuration=dev-local-libs",
    "test:forms": "ng test forms",
    "test:forms:browsers": "ng test forms --watch=false --browsers=Chrome,IE11,Edge",
    "test:forms:ci": "ng test forms --watch=false --progress=false --source-map=false --browsers=ChromeHeadlessCI",
    "test:browsers": "run-s test:forms:browsers",
    "test:ci": "run-s test:forms:ci",
    "test": "run-s test:forms",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "precise-commits": "precise-commits",
    "lint-staged": "lint-staged",
    "webpack-bundle-analyzer": "webpack-bundle-analyzer dist/for-report/sandbox/stats.json",
    "build:prod-bundle-report": "ng build --prod --source-map --stats-json --output-hashing=none --output-path=dist/for-report/sandbox",
    "stats:webpack-bundle-analyzer": "run-s build:prod-bundle-report webpack-bundle-analyzer",
    "source-map-explorer": "source-map-explorer dist/for-report/sandbox/main.js",
    "stats": "run-s build:prod-bundle-report source-map-explorer",
    "e2e:sandbox": "./node_modules/.bin/cypress open --project=apps/sandbox",
    "e2e": "run-s e2e:sandbox"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint --project tsconfig.json --exclude **/*.d.ts"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.1",
    "@angular/cdk": "^7.1.1",
    "@angular/common": "^7.1.1",
    "@angular/compiler": "^7.1.1",
    "@angular/core": "^7.1.1",
    "@angular/forms": "^7.1.1",
    "@angular/platform-browser": "^7.1.1",
    "@angular/platform-browser-dynamic": "^7.1.1",
    "@angular/router": "^7.1.1",
    "@clr/angular": "^1.0.0",
    "@clr/icons": "^1.0.0",
    "@clr/ui": "^1.0.0",
    "@nrwl/nx": "7.1.1",
    "@webcomponents/custom-elements": "^1.2.1",
    "core-js": "^2.6.0",
    "date-fns": "^2.0.0-alpha.25",
    "lib": "^3.0.2",
    "ngx-file-drop": "^5.0.0",
    "ramda": "^0.26.1",
    "rxjs": "^6.3.3",
    "typescript-memoize": "^1.0.0-alpha.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.2",
    "@angular-devkit/build-ng-packagr": "~0.11.2",
    "@angular/cli": "~7.1.2",
    "@angular/compiler-cli": "^7.1.1",
    "@angular/language-service": "^7.1.1",
    "@ngrx/store-devtools": "^6.1.2",
    "@nrwl/schematics": "7.1.1",
    "@types/jasmine": "~2.8.9",
    "@types/jasminewd2": "~2.0.5",
    "@types/node": "~10.12.10",
    "@types/ramda": "0.25.39",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "~4.5.0",
    "cypress": "^3.1.2",
    "electron": "^3.0.10",
    "husky": "^1.2.0",
    "jasmine-core": "~3.3.0",
    "jasmine-marbles": "0.4.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-edge-launcher": "^0.4.2",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "lint-staged": "^8.1.0",
    "ng-packagr": "^4.4.5",
    "ngrx-store-freeze": "0.2.4",
    "npm-run-all": "^4.1.5",
    "precise-commits": "^1.0.2",
    "prettier": "1.15.3",
    "protractor": "~5.4.1",
    "ts-node": "~7.0.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "typescript": "3.1.6",
    "wallaby-webpack": "^3.9.13",
    "webpack-bundle-analyzer": "^3.0.3",
    "source-map-explorer": "^1.6.0",
    "@nrwl/builders": "7.1.1"
  }
}
